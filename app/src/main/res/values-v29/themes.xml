        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorPrimaryDark</item>
        <item name="colorOnPrimary">@color/honey</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/colorPrimaryComplementry</item>
        <item name="colorSecondaryVariant">@color/colorPrimaryComplementryDark</item>
        <item name="colorOnSecondary">@color/honey</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:forceDarkAllowed">false</item>
    </style>
</resources>